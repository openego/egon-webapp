{
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(115,62,136)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    }
  },
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(115,62,136)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-variable-anchor": [
        "center",
        "top"
      ],
      "text-radial-offset": 2,
      "text-field": "{name}",
      "text-font": [
        "Flexo Medium"
      ],
      "text-letter-spacing": 0.04
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85,
      "text-halo-blur": 1.5
    }
  },
  "demand_cts": {
    "type": "fill",
    "paint": {
      "fill-color": [
        "step",
        ["get","demand"],
        "#00eda0", 500,
        "#00eda0", 1000,
        "#0ed976", 5000,
        "#0eb24c", 10000,
        "#0d8d3c", 50000,
        "#0c4e2a", 100000,
        "#031a1c", 500000,
        "hsl(348, 100%, 37%)", 1000000,
        "#0d0026"
      ],
      "fill-opacity": 0.8
    }
  },
  "demand_household": {
    "type": "fill",
    "paint": {
      "fill-color": [
        "step",
        ["get","demand"],
        "#ffeda0", 500,
        "#ffeda0", 1000,
        "#fed976", 5000,
        "#feb24c", 10000,
        "#fd8d3c", 50000,
        "#fc4e2a", 100000,
        "#e31a1c", 500000,
        "hsl(348, 100%, 37%)", 1000000,
        "#bd0026"
      ],
      "fill-opacity": 0.8
    }
  },
  "supply_biomass": {
    "type": "symbol",
    "layout": {
      "icon-image": "biomass",
      "icon-size": 0.5
    }
  },
  "supply_run_of_river": {
    "type": "symbol",
    "layout": {
      "icon-image": "river",
      "icon-size": 0.5
    }
  },
  "supply_wind": {
    "type": "symbol",
    "layout": {
      "icon-image": "wind",
      "icon-size": 0.5
    }
  },
  "supply_solar": {
    "type": "symbol",
    "layout": {
      "icon-image": "solar",
      "icon-size": 0.5
    }
  },
  "potential_wind": {
    "type": "fill",
    "paint": {
      "fill-color": "darkblue",
      "fill-opacity": 0.8
    }
  },
  "potential_pv": {
    "type": "fill",
    "paint": {
      "fill-color": "gold",
      "fill-opacity": 0.8
    }
  },
  "ehv_line": {
    "type": "line",
    "paint": {
      "line-color": "blue"
    }
  },
  "hv_line": {
    "type": "line",
    "paint": {
      "line-color": "darkblue"
    }
  },
  "ehv_hv_station": {
    "type": "symbol",
    "layout": {
      "icon-image": "station",
      "icon-size": 0.5
    }
  },
  "hv_mv_station": {
    "type": "symbol",
    "layout": {
      "icon-image": "station",
      "icon-size": 0.8
    }
  },
  "mv_grid_districts": {
    "type": "fill",
    "paint": {
      "fill-color": [
        "step",
        ["get","area"],
        "#00eda0", 100000,
        "#00eda0", 1000000,
        "#00d976", 10000000,
        "#001a1c", 100000000,
        "hsl(348, 100%, 37%)", 1000000000,
        "#bd0026"
      ],
      "fill-opacity": 0.8
    }
  },
  "soil_quality": {
    "continuous": "True",
    "from": [0, 0, 0],
    "to": [200, 140, 0]
  }
}
