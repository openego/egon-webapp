{
  "region-line": {
    "type": "line",
    "paint": {
      "line-color": "rgb(115,62,136)",
      "line-width": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        3,
        0.25
      ]
    }
  },
  "region-fill": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(115,62,136)",
      "fill-opacity": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        0,
        0.1
      ]
    }
  },
  "region-label": {
    "type": "symbol",
    "layout": {
      "text-variable-anchor": [
        "center",
        "top"
      ],
      "text-radial-offset": 2,
      "text-field": "{name}",
      "text-font": [
        "Flexo Medium"
      ],
      "text-letter-spacing": 0.04
    },
    "paint": {
      "text-color": [
        "case",
        [
          "boolean",
          [
            "feature-state",
            "hover"
          ],
          false
        ],
        "#222222",
        "#000000"
      ],
      "text-halo-color": "hsl(0, 0%, 100%)",
      "text-halo-width": 0.85,
      "text-halo-blur": 1.5
    }
  },
  "demandcts": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(0, 100, 0)",
      "fill-opacity": 0.8
    }
  },
  "demandhousehold": {
    "type": "fill",
    "paint": {
      "fill-color": [
        "step",
        ["get","demand"],
        "#ffeda0", 500,
        "#ffeda0", 1000,
        "#fed976", 5000,
        "#feb24c", 10000,
        "#fd8d3c", 50000,
        "#fc4e2a", 100000,
        "#e31a1c", 500000,
        "hsl(348, 100%, 37%)", 1000000,
        "#bd0026"
      ],
      "fill-opacity": 0.8
    }
  },
  "cluster": {
    "type": "fill",
    "paint": {
      "fill-color": "rgb(100, 200, 100)",
      "fill-opacity": 0.8
    }
  },
  "hospital": {
    "type": "symbol",
    "layout": {
      "icon-image": "hospital",
      "icon-size": 0.5
    }
  },
  "hospital_simulated": {
    "type": "symbol",
    "layout": {
      "icon-image": "hospital",
      "icon-size": 1.0
    }
  }
}
